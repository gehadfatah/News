<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_news"
    app:startDestination="@id/newsFragmnt">
    <fragment
        android:id="@+id/newsFragmnt"
        android:name="com.goda.newstk.features.news.presentation.ui.fragments.NewsFragment"
        android:label="News"
        tools:layout="@layout/fragment_news">
        <action
            android:id="@+id/action_characters_to_search"
            app:destination="@+id/detail"
            app:enterAnim="@anim/nav_default_enter_anim"
            app:exitAnim="@anim/nav_default_exit_anim" />

    </fragment>
    <fragment
        android:id="@+id/detail"
        android:name="com.goda.newstk.features.news.presentation.ui.fragments.NewsFragment"
        android:label="null"
        tools:layout="@layout/activity_news_detail">

    </fragment>
</navigation>